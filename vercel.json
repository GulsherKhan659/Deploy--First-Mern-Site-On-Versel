{
    "version": 2,
    "name": "mern-app",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/blog",
            "dest": "index.js"
        },
        {
            "src": "/blog/(.*)",
            "dest": "index.js"
        }
    ]
}